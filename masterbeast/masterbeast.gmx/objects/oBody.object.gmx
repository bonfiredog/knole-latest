    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Movement And Shaking

//Moving with necks, and rotating depending on movement.

x = oNecks.x + lengthdir_x(Distance,oNecks.image_angle + Angle)
y = oNecks.y + lengthdir_y(Distance,oNecks.image_angle + Angle)
image_angle = oNecks.image_angle + RotateMod

RotateMod = clamp(RotateMod,-30,30)

if oSarahHead.PickedUp = true {
if mInterfaceController.MoveSpeed &gt; 15 and mInterfaceController.VMove = "none" {
if mInterfaceController.HMove = "left" {
SwingReady = true
RotateMod += (15 / mInterfaceController.SpeedMax) * mInterfaceController.CurrentSpeed
} else if mInterfaceController.HMove = "right" {
SwingReady = true
RotateMod -= (15 / mInterfaceController.SpeedMax) * mInterfaceController.CurrentSpeed
} 
}

//Continuing movement (a swing) for a time once stopped moving.
if mInterfaceController.HMove = "none" and SwingReady = true {
SwingReady = false
Swinging = true
SwingTimer = 15
if RotateMod &gt; 0 {
SwingMod = -1
} else if RotateMod &lt; 0 {
SwingMod = 1
}
}

if mInterfaceController.HMove = "none" {
if Swinging = true {
if SwingTimer &gt; 0 {
SwingTimer -= 1
RotateMod += (10 * SwingMod)
} else if SwingTimer &lt;= 0 {
if RotateMod &gt; 0 {
RotateMod -= 4
}
if RotateMod &lt; 0 {
RotateMod += 4
}
Swinging = false
}
} else if Swinging = false {
if RotateMod &gt; 0 {
RotateMod -= 3
}
if RotateMod &lt; 0 {
RotateMod += 3
}
}
}
}

//Pepper Spray

if oSarahHead.Rooted = false {
if mInterfaceController.VMove = "down" and mInterfaceController.MoveSpeed &gt; 20 and Shake = false {
Shake = true
SavedSpeed = mInterfaceController.MoveSpeed
}

if mInterfaceController.VMove = "none" and Shake = true {
PepperX = oReplaceDetect.x
PepperY = oReplaceDetect.y
SavedX = PepperX
SavedY = PepperY + 140
part_emitter_region(PepperSys,PepperEmit,PepperX - 10,PepperX + 10,PepperY - 10,PepperY + 10,ps_shape_ellipse,ps_distr_gaussian)
part_emitter_burst(PepperSys,PepperEmit,PepperPart,round(0.5 * SavedSpeed))
ShakeSound = choose("peppershake1","peppershake2","peppershake3","peppershake4","peppershake5")
ex_audio_set_position(ShakeSound,mouse_x,mouse_y,1)
ex_audio_set_volume(ShakeSound,BothVolMod(0.3),0)
ex_audio_set_pitch(ShakeSound,random_range(0.8,1.2),0)
ex_audio_play(ShakeSound,0)
WriteToLog("Pepper shook.")
alarm[0] = 32
Shake = false
}
}

FadeInWithZoom()

</string>
          </argument>
