            <kind>1</kind>
            <string>///Position

if mDebug.StartScrollSystem = true and MarkerSet = false and mDebug.FullyOpen = true {

ScrollButtonTopY = mDebug.Column2TopY + 20
ScrollButtonBottomY = mDebug.Column2TopY + 1300 - 20
ScrollButtonX = mDebug.Column1LeftX - 50
YDiff = ScrollButtonBottomY - ScrollButtonTopY
x = ScrollButtonX
y = ScrollButtonTopY
MarkerSet = true 
}


if MarkerSet = true {
x = ScrollButtonX




if mouse_wheel_up() {
y -= 25


if y &gt;= ScrollButtonBottomY {
y = ScrollButtonBottomY 
} 

if y &lt;= ScrollButtonTopY {
y = ScrollButtonTopY
}
}

if mouse_wheel_down() {
y += 25


if y &gt;= ScrollButtonBottomY {
y = ScrollButtonBottomY 
} 

if y &lt;= ScrollButtonTopY {
y = ScrollButtonTopY
}
}

if keyboard_check(vk_up) {
y -= 25

if y &gt;= ScrollButtonBottomY {
y = ScrollButtonBottomY 
} 

if y &lt;= ScrollButtonTopY {
y = ScrollButtonTopY
}
}

if keyboard_check(vk_down) {
y += 25

if y &gt;= ScrollButtonBottomY {
y = ScrollButtonBottomY 
} 

if y &lt;= ScrollButtonTopY {
y = ScrollButtonTopY
}
}


if DebugCursorTouching() and mouse_check_button(mb_any) {
if mouse_y &gt; ScrollButtonTopY and mouse_y &lt; ScrollButtonBottomY {
y = mouse_y
}

if mouse_y &gt;= ScrollButtonBottomY {
y = ScrollButtonBottomY 
} 

if mouse_y &lt;= ScrollButtonTopY {
y = ScrollButtonTopY
}

}

y = clamp(y,ScrollButtonTopY,ScrollButtonBottomY)

}

</string>
