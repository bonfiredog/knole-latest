            <kind>1</kind>
            <string>///Clipping Mask

//Check if main surface exists.
if !surface_exists(surf) then surf = surface_create(size,size);

//Create a temp mask surface.
var mask_surf = surface_create(size,size);

surface_set_target(mask_surf); //Set surface target to mask.
    ClippingMask_madgetr(true); //Call this to start the clipping mask build.
    
        //All drawing in here will be on the clipping mask.
        draw_set_font(fntMain);
        draw_set_colour(c_white);
        draw_set_halign(fa_center);
        draw_text(size*0.5,size*0.75,"Trevor Madge Clipping Mask");
        draw_sprite_ext(sprite_index,0,size*0.5,size*0.5,image_xscale,image_yscale,image_angle,image_blend,image_alpha);
        //
        
    ClippingMask_madgetr(false); //Call this to reset from the clipping mask.
surface_reset_target();

surface_set_target(surf); //Set surface target to main surface.

draw_clear_alpha(c_white,0); //Clear it to start.

//Draw what we want drawn through the clipping mask.
with(oSplatter)
{ 
    draw_sprite_ext(sprite_index,image_index,x-other.x+(other.size*0.5),y-other.y+(other.size*0.5),image_xscale,image_yscale,image_angle,image_blend,image_alpha);
};

//Clip the surface using the clipping mask.
draw_set_blend_mode(bm_subtract);
draw_surface(mask_surf,0,0);
draw_set_blend_mode(bm_normal);

surface_reset_target();

//Free mask from storage.
surface_free(mask_surf);
</string>
