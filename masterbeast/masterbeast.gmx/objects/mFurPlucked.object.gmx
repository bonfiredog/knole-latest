            <string>///Shedding Movement

if Shedding = true {
speed = (750 / room_speed) * ShedDirMod

if ShedDir = "left" {

//Ease the Mod
if direction &gt;= 250 {
ShedDirMod = abs((270 - direction)) / 20
} else if direction &lt; 250 and direction &gt; 210 {
ShedDirMod = 1
} else if direction &lt;= 210 {
ShedDirMod = abs((190 - direction)) / 20
}

if direction &gt; 190 {
direction -= 1
} else if direction &lt;= 190 {
ShedDir = "right" 
direction = 270
}
} else if ShedDir = "right" {

if direction &lt;= 290 {
ShedDirMod = abs((270 - direction)) / 20
} else if direction &gt; 290 and direction &lt; 330 {
ShedDirMod = 1
} else if direction &gt;= 330 {
ShedDirMod = (350 - direction) / 20
}

if direction &lt; 350 {
direction += 1
} else if direction &gt;= 350 {
ShedDir = "left" 
direction = 270
}
}

}


</string>
