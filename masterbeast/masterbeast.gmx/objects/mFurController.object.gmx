            <string>///Controlling Wind Movement

if WindLife &gt; 0 {
WindLife -= 1 * mWeatherSubController.SimpleWindS
} else if WindLife &lt;= 0 {
WindLife = random_range(20,60)
if WindDir = "left" {
WindDir = "right"
} else if WindDir = "right" {
WindDir = "left"
}
}

//--------------------------------------

if mWeatherSubController.SimpleWindS &gt; 0 {
with mFurParent {
if mFurController.WindDir = "left" {
WindMod -= (((0.05 * mWeatherSubController.SimpleWindS)) - (0.4 / 100) * WetTouchPercent)
} else if mFurController.WindDir = "right" {
WindMod += (((0.05 * mWeatherSubController.SimpleWindS)) - (0.4 / 100) * WetTouchPercent)
}
WindMod = clamp(WindMod,-25,25)
}
with mFurCurledParent {
if mFurController.WindDir = "left" {
WindMod -= (((0.06 * mWeatherSubController.SimpleWindS)) - (0.4 / 100) * WetTouchPercent)
} else if mFurController.WindDir = "right" {
WindMod += (((0.06 * mWeatherSubController.SimpleWindS)) - (0.4 / 100) * WetTouchPercent)
}
WindMod = clamp(WindMod,-25,25)
}
with mFurBrowParent {
if mFurController.WindDir = "left" {
WindMod -= (((0.06 * mWeatherSubController.SimpleWindS)) - (0.4 / 100) * WetTouchPercent)
