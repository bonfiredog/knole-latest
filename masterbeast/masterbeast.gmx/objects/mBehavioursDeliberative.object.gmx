        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Disappearing &amp; Reappearing

if PercentForward &lt; 100 {
if LessThanPresent = false {
LessThanPresent = true
if instance_exists(DScreen) = false {
DScreen = instance_create(room_width / 2,room_height / 2,mDisappearScreen)
}
}
} else {
if LessThanPresent = true {
LessThanPresent = false
view_wview[0] = 1400
view_hview[0] = 1920
view_xview[0] = 0
view_yview[0] = 1000
mWorldController.ViewportMinScale = 0.9
mWorldController.ViewportMaxScale = 1.14
if instance_exists(DScreen) {
with DScreen {
instance_destroy()
}
}
}
}

if LessThanPresent = true {
view_wview[0] = 1400 + ((1199 / 100) * (100 - PercentForward))
view_hview[0] = 1920 + ((1199 / 100) * (100 - PercentForward))
mInterfaceController.BaseXView = 0 - ((1250 / 100) * (100 - PercentForward))
mInterfaceController.BaseYView = 1000 - ((1000 / 100) * (100 - PercentForward))
mWorldController.ViewportMinScale = 0.9 + ((1 / 100) * (100 - PercentForward))
mWorldController.ViewportMaxScale = 1.14 + ((1 / 100) * (100 - PercentForward))

with mWorldController {
CurrentViewportScale = clamp(CurrentViewportScale,ViewportMinScale,ViewportMaxScale)
}
}

//Disappearing In One Go
if DisappearingActive = true {

if StartDisappearing = false {
mDriveCollectionMaster.CurrentlyBreathing = false

//Come Forward First
if mInterfaceController.CurrentView = MacroView {
if PercentForward &lt; 100 {
PercentForward += 0.4
}
//Resetting The View
} else if mInterfaceController.CurrentView = MicroView {
StartZoom(50,mouse_x,mouse_y)
}

if mInterfaceController.CurrentView = MacroView and PercentForward &gt;= 100 {
ThisViewX = mInterfaceController.BaseXView
ThisViewY = mInterfaceController.BaseYView
if ThisViewX != 0 {
if ThisViewX &lt; -FocusSpeed {
mInterfaceController.BaseXView += FocusSpeed
} else if ThisViewX &gt; FocusSpeed {
mInterfaceController.BaseXView -= FocusSpeed
} else {
mInterfaceController.BaseXView = 0
}
}
if ThisViewY != 1000 {
if ThisViewY &lt; 1000 - FocusSpeed {
mInterfaceController.BaseYView += FocusSpeed
} else if ThisViewY &gt; (1000 + FocusSpeed)  {
mInterfaceController.BaseYView -= FocusSpeed
} else {
mInterfaceController.BaseYView = 1000
}
}


if StartDSneeze = false {
StartSneezeBehaviour(1,100,1,false)
StartDSneeze = true
} else if StartDSneeze = true {
if StartDisappearing = false {
oNose1.Redness += 0.001
if mBehavioursReactive.SneezeActive = true and mBehavioursReactive.SneezePos = "out" {
StartDisappearing = true
}
}
}
}
} else if StartDisappearing = true {
if BlownNose = false and PercentForward &lt; 100 {
with DScreen {
part_emitter_region(DisPartSys,DisEmitLeft,oBLNostril.x - 5,oBLNostril.x +5,
oBLNostril.y - 5,oBLNostril.y + 5,ps_shape_ellipse,ps_distr_invgaussian)
part_emitter_burst(DisPartSys,DisEmitLeft,DisPart,1000)
part_emitter_region(DisPartSys,DisEmitRight,oBRNostril.x - 5,oBRNostril.x +5,
oBRNostril.y - 5,oBRNostril.y + 5,ps_shape_ellipse,ps_distr_invgaussian)
part_emitter_burst(DisPartSys,DisEmitRight,DisPart,1000)
}
ex_audio_set_volume("lowrumblerf",0.05,0)
ex_audio_set_pitch("lowrumblerf",random_range(0.8,0.9))
ex_audio_play("lowrumblerf",1)
BlownNose = true 
}
StartScreenShake(3,10,1)
if PercentForward &gt; 0 {
PercentForward -= 3
} else if PercentForward = 0 {
ex_audio_set_volume("lowrumblerf",0,30)
BlownNose = false
DisappearingActive = false
StartDisappearing = false
mDriveCollectionMaster.CurrentlyBreathing = true
WriteToLog("Disappeared with sneeze.")
}
} 
}



if mInterfaceController.SequenceBegun = true {
if IsInRitual() = false {
StartShiverBehaviour(0.3,40,10)
CantEscapeReactions()
}
MovingActive = false
} else {
if mInterfaceController.CurrentView = MacroView {
if MovingActive = true and mInterfaceController.ChangingView = false {
switch MovingDir {
case "retreat":
if PercentForward &gt; 0 {
if MoveCalc = false {
MovingTimer = 0
MovingStart = PercentForward
if MovingChange &gt; 5 {
ex_audio_set_volume("lowrumblerf",0.05,0)
ex_audio_set_pitch("lowrumblerf",random_range(0.8,0.9))
ex_audio_play("lowrumblerf",1)
}
MoveCalc = true
} else if MoveCalc = true {
if PercentForward &gt; 0 {
if MovingTimer &lt; MovingSpeed {
MovingTimer += 1
PercentForward = ease_inout_sine(MovingTimer, MovingStart, -MovingChange,MovingSpeed)
} else { 
ex_audio_set_volume("lowrumblerf",0,5)
MovingActive = false
}
} else {
ex_audio_set_volume("lowrumblerf",0,5)
MovingActive = false
}
}
} else { 
ex_audio_set_volume("lowrumblerf",0,5)
MovingActive = false
}
break;
case "comeforward":
if PercentForward &lt; 100 {
if MoveCalc = false {
MovingTimer = 0
MovingStart = PercentForward
if MovingChange &gt; 5 {
ex_audio_set_volume("lowrumblerf",0.05,0)
ex_audio_set_pitch("lowrumblerf",random_range(0.8,0.9))
ex_audio_play("lowrumblerf",1)
}
MoveCalc = true
} else if MoveCalc = true {
if PercentForward &lt; 100 {
if MovingTimer &lt; MovingSpeed {
MovingTimer += 1
PercentForward = ease_inout_sine(MovingTimer,MovingStart,MovingChange,MovingSpeed)
} else { 
//ex_audio_set_volume("lowrumblerf",0,5)
ex_audio_stop("lowrumblerf")
MovingActive = false
}
} else {
//ex_audio_set_volume("lowrumblerf",0,5)
ex_audio_stop("lowrumblerf")
MovingActive = false
}
}
} else {
//ex_audio_set_volume("lowrumblerf",0,5)
ex_audio_stop("lowrumblerf")
MovingActive = false
}
break;
}
} else {
//ex_audio_set_volume("lowrumblerf",0,5)
ex_audio_stop("lowrumblerf")
MovingActive = false
} 
} else {
//ex_audio_set_volume("lowrumblerf",0,5)
ex_audio_stop("lowrumblerf")
MovingActive = false
}
}




if ex_audio_is_playing("lowrumblerf") {
if ex_audio_get_volume("lowrumblerf") &lt;= 0 {
ex_audio_stop("lowrumblerf")
}
}

PercentForward = clamp(PercentForward,0,100)

