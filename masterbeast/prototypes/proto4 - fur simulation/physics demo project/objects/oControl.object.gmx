            <string>//Every 5 steps, if the left mouse button is pressed,
//spawn a random shape at the mouse location.

if (mouse_check_button(mb_left) &amp;&amp; (alarm[0] &lt; 0))
{
instance_create(mouse_x, mouse_y, choose(oBlock, oCircle, oTriangle, oStar));
alarm[0] = 5;
}

//Quit Game

if keyboard_check(ord('Q')) {
game_end()
}

//When the right mouse button is pressed,
//Apply an impulse to every shape in the room
//Of intensity 8, from the mouse position towards itself.

if mouse_check_button(mb_right)
{
with (oDynamicParent)
    {
    dir = point_direction(mouse_x, mouse_y, x, y);
    physics_apply_impulse(x, y, lengthdir_x(8, dir), lengthdir_y(8, dir));
    }
}

</string>
