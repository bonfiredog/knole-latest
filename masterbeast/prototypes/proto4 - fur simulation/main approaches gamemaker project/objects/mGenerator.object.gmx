instance_destroy()
with ThisHair{instance_destroy()}
} else {
mGenerator.NumberOfPoresCreated = mGenerator.NumberOfPoresCreated + 1
} 
}

} else if NumberOfPoresCreated &gt;= NumberOfPoresToCreate {
AllPoresCreated = true
}
} else if AllPoresCreated = true {

