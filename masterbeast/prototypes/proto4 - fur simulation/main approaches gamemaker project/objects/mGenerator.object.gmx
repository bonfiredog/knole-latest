y = random_range(0, room_height)
direction = mGenerator.FurDirection
image_angle = random(360)
if place_meeting(x,y,oPore) or place_meeting(x,y,oPoreEmpty) {
instance_destroy()
} else {
mGenerator.NumberOfEmptyPoresCreated = mGenerator.NumberOfEmptyPoresCreated + 1
} 
}
} else if NumberOfEmptyPoresCreated &gt;= NumberOfEmptyPoresToCreate {
AllEmptyPoresCreated = true
}







